{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true,
    "node": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "globals": {
    "mockEstimateFaces": "readonly"
  },
  "rules": {
    "no-unused-vars": ["warn"],
    "no-console": ["off"]
  }
}
